{
  "name": "zrender",
  "version": "4.2.0",
  "description": "A lightweight canvas library.",
  "keywords": [
    "canvas",
    "2d"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ecomfe/zrender.git"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run build:bundle && npm run build:cjs && npm run build:esm",
    "build:bundle": "node build/build.js",
    "build:cjs": "node_modules/.bin/tsc -m CommonJS --outDir lib",
    "build:esm": "node_modules/.bin/tsc -m ES2015 --outDir esm",

    "watch:esm": "node_modules/.bin/tsc -w -m ES2015 --outDir esm"
  },
  "license": "BSD-3-Clause",
  "dependencies": {
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.7.2",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "commander": "2.11.0",
    "eslint": "6.3.0",
    "fs-extra": "4.0.2",
    "rollup": "^1.28.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "rollup-plugin-uglify": "^6.0.4",
    "typescript": "^3.7.4"
  }
}
